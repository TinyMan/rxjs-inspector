{
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "js": ["js/content_script.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools.html",
  "description":
    "DevTools for inspecting and debugging RxJS applications (goes with @rxjs-inspector)",
  "homepage_url": "https://github.com/TinyMan/rxjs-inspector",
  "icons": {},
  "manifest_version": 2,
  "name": "RxJS Inspector DevTools",
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["*://localhost/*"]
  },
  "permissions": [],
  "short_name": "RxJS Inspector DevTools",
  "version": "0.0.0-PLACEHOLDER"
}
