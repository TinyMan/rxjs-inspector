{
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "bootstrap": "lerna bootstrap",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json **/*.ts --exclude node_modules/**/*"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^22.4.3",
    "lerna": "^2.11.0",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "rxjs-tslint": "^0.1.3",
    "rxjs-tslint-rules": "^4.0.2",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "name": "rxjs-inspector",
  "version": "0.0.0",
  "description": "monorepo for rxjs-inspector",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TinyMan/rxjs-inspector.git"
  },
  "keywords": [
    "rxjs",
    "inspector",
    "debug"
  ],
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "tslint -p tsconfig.json --fix",
      "git add"
    ]
  },
  "author": "TinyMan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TinyMan/rxjs-inspector/issues"
  },
  "homepage": "https://github.com/TinyMan/rxjs-inspector#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "packages/**/*.{js,ts}",
      "!**/*.d.ts",
      "!**/lib/**",
      "!**/node_modules/**"
    ]
  }
}
