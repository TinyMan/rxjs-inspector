{
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json **/*.ts --exclude node_modules/**/*",
    "watch:extension": "lerna run start --scope @rxjs-inspector/extension --stream",
    "build:extension": "lerna run build --scope @rxjs-inspector/extension --include-filtered-dependencies",
    "build:panel": "lerna run build --scope @rxjs-inspector/panel --include-filtered-dependencies",
    "bw:panel": "ng build panel --configuration watch",
    "watch:panel": "ng serve panel",
    "build:demo": "ng build demo-app",
    "watch:demo": "ng serve demo-app"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/config-lerna-scopes": "^6.1.3",
    "@ngrx/store-devtools": "^5.2.0",
    "@types/chrome": "0.0.64",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.1",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.3",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lerna": "^2.11.0",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "protractor": "~5.3.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "rxjs-marbles": "^4.1.0",
    "rxjs-tslint": "^0.1.3",
    "rxjs-tslint-rules": "^4.0.2",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.7.0",
    "webpack-chrome-extension-reloader": "^0.6.7",
    "webpack-cli": "^2.1.2",
    "webpack-merge": "^4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "name": "rxjs-inspector",
  "version": "0.0.0",
  "description": "monorepo for rxjs-inspector",
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/entity": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "core-js": "^2.5.4",
    "immutable": "^3.8.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TinyMan/rxjs-inspector.git"
  },
  "keywords": [
    "rxjs",
    "inspector",
    "debug"
  ],
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "tslint -p tsconfig.json --fix",
      "git add"
    ]
  },
  "author": "TinyMan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TinyMan/rxjs-inspector/issues"
  },
  "homepage": "https://github.com/TinyMan/rxjs-inspector#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "packages/**/*.{js,ts}",
      "!**/*.d.ts",
      "!**/lib/**",
      "!**/node_modules/**",
      "!**/webpack*.js",
      "!packages/panel/**/*",
      "!packages/extension/**/*"
    ],
    "testPathIgnorePatterns": [
      "/demo-app/",
      "packages/panel/",
      "packages/extension/"
    ],
    "moduleNameMapper": {
      "^@rxjs-inspector/([^/]+)(/.+)": "<rootDir>/packages/$1/src$2",
      "^@rxjs-inspector/(.*)": "<rootDir>/packages/$1/src"
    }
  }
}
